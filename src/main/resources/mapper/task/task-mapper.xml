<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="TaskDAO">

    <select id="selectTaskMembList" resultType="TaskMembDTO">
        SELECT
             MEMB_UNQ
            ,MEMB_DEPT_NM
            ,MEMB_NM
            ,MEMB_PST_NM
            ,MEMB_TEL
            ,MEMB_EMAIL
            ,REG_USR_ID
            ,REG_DTM
            ,CHG_USR_ID
            ,CHG_DTM
            ,DEL_YN
        FROM
            TBL_TASK_MEMB_LST
    </select>

</mapper>